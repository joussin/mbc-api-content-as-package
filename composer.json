{
    "name": "joussin/mbc-api-content-as-package",
    "description": "Create an api content manager with backoffice",
    "license": "MIT",
    "keywords": [
        "spatie",
        "laravel-export"
    ],
    "homepage": "https://github.com/joussin/mbc-api-content-as-package",
    "authors": [
        {
            "name": "Joussin St√©phane",
            "email": "joussin@live.com",
            "homepage": "https://github.com/joussin/",
            "role": "Developer"
        }
    ],
    "repositories": [

        {
            "type": "vcs",
            "url": "https://github.com/joussin/laravel-export.git"
        }

    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^8.73|^9.0",
        "illuminate/contracts": "^8.73|^9.0",
        "illuminate/http": "^8.73|^9.0",
        "illuminate/support": "^8.73|^9.0",
        "psr/http-message": "^1.0",
        "symfony/console": "^5.3|^6.0",
        "symfony/dom-crawler": "^5.3|^6.0",
        "symfony/http-foundation": "^5.3|^6.0",
        "symfony/process": "^5.3|^6.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "joussin/laravel-export": "0.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MbcApiContent\\Providers\\MainServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "MbcApiContent\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MbcApiContent\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }

}
